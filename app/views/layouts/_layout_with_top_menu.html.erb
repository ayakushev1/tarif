<div id="container" class="container-fluid">
	<div class="row">
		<div class="col-xs-12">
			<div class="navbar navbar-inverse" role="navigation">
				<%= render 'users/sessions/current_user_info'%>
			</div>
			
			<div class="navbar navbar-default hidden-xs" role="navigation">
		  		<%= render('/application/menus/operator_menu') %>
			</div>
			<div>
				<%= render('/application/menus/main_top_menu') %> 
			</div>	
		</div>	
	</div>
	<div class="row">
        <div class="col-xs-12">
        	
			<ol class="breadcrumb">
			  <li><%= yield(:title).blank? ? 'Title' : yield(:title) %></li>
			</ol>        	

        	<div id="main_block_top_row_4" class="content">
				<%= render('/application/shared/alerts') if false %>
				<%= "user_signed_in? #{user_signed_in?}" if false %>
				<%= "current_user #{current_user}" if false %>
				<%= "user_session #{user_session}" if false %>
				<%= "current_or_guest_user #{current_or_guest_user}" if false %>
				<%= User.where(:id => current_or_guest_user.id).first[:password_digest].blank? if false %>

				<%= render :partial => 'application/shared/alerts' %>
				
				<div id=<%= view_id_name %> >
					  <%= yield %>
				</div>	  
        	</div>
        	<br />
	        <div>
				<%= render('/application/shared/yandex_metrika_informet') if ENV["RAILS_ENV"] == "production" %>
	        </div>
        </div>
	</div>
</div>

