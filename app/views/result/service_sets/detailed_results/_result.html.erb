<%
body = if result_service_sets.model.first.run.comparison_group
	[
		content_tag(:strong, "Название рейтинга: ") + "#{result_service_sets.model.first.run.comparison_group.optimization.name}",
		content_tag(:strong, "Название корзины: ") + "#{result_service_sets.model.first.run.comparison_group.name}",
	]
else
	[
		content_tag(:strong, "Сохраненное название подбора: ") + "#{result_service_sets.model.first.run.name}",
		content_tag(:strong, "Сохраненное название звонков: ") + "#{result_service_sets.model.first.run.call_run.name}",
	]
end

panel = {
	:head => "Описание подбора тарифа",
	:body => body,
	:footer => render( :partial => 'result/service_sets/detailed_results/information') + 
		link_to("Назад", service_sets_detailed_results_return_link_to, 
			{:remote => true, :class => "btn btn-success", :type =>"button", :rel => "nofollow"}),
}
%>

<%= render :partial => "application/layouts/panel", :locals => {:panel => panel} %>
